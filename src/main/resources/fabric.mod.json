{
  "schemaVersion": 1,
  "id": "songplayer-addon",
  "version": "${version}",
  "name": "Song Player Addon",
  "description": "A Song Player addon for Meteor Client.",
  "authors": [
    "hotian"
  ],
  "contact": {
    "sources": "https://github.com/hotian/meteor-addon-songplayer"
  },
  "icon": "assets/songplayer/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "hotian.addon.songplayer.SongPlayerAddon"
    ]
  },
  "mixins": [
    "songplayer.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.8"],
    "meteor-client": "*"
  }
}
